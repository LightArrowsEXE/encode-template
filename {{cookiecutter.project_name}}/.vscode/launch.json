{
    "configurations": [
        {
            "name": "VS Preview Current File",
            "module": "vspreview",
            "args": [
                "${file}"
            ],
            "type": "debugpy",
            "request": "launch",
            "console": "internalConsole",
            "gevent": false,
            "justMyCode": true,
            "logToFile": false,
            "subProcess": false,
            "redirectOutput": true,
            "showReturnValue": false,
            "suppressMultipleSessionWarning": false
        },
        {
            "name": "Run Current File",
            "program": "${file}",
            "type": "debugpy",
            "request": "launch",
            "console": "internalConsole",
            "gevent": false,
            "justMyCode": true,
            "logToFile": false,
            "subProcess": false,
            "redirectOutput": true,
            "showReturnValue": false,
            "suppressMultipleSessionWarning": false
        }
    ]
}